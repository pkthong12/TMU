{
  "name": "alpha-frontend-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:ALPHA": "ng serve ALPHA_ROOT --open --ssl true --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "start:ALPHA-P": "ng serve ALPHA_PORTAL --open --ssl true --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "start:VEAM": "ng serve VEAM --open --ssl true --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "start:VINATEX": "ng serve VINATEX --open --ssl true --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "start:VNS": "ng serve VNS --open --ssl true --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "start:VNS-P": "ng serve VNS-PORTAL --open --ssl true --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "start:ALPHA:prod": "npx http-server -p 8081 -c-1 dist/veam/browser",
    "start:VEAM:prod": "npx http-server -p 8081 -c-1 dist/veam/browser",
    "start:VINATEX:prod": "npx http-server -p 8081 -c-1 dist/vinatex/browser",
    "start:VNS:prod": "npx http-server -p 8081 -c-1 dist/vns/browser",
    "start:VNS-P:prod": "npx http-server -p 8081 -c-1 dist/vns-portal/browser",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:w": "ng build alpha-global-constants --watch",
    "build:lib-p": "ng build alpha-global-constants --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:VNS-PORTAL": "node dist/vns-portal/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.2.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.2.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/service-worker": "^17.0.0",
    "@ckeditor/ckeditor5-angular": "^7.0.1",
    "@ckeditor/ckeditor5-build-classic": "^41.0.0",
    "@microsoft/signalr": "^7.0.14",
    "@swimlane/ngx-charts": "^20.5.0",
    "alpha-global-constants": "^0.1.2",
    "angular-highcharts": "^16.0.0",
    "bootstrap": "^5.3.2",
    "highcharts": "^11.3.0",
    "highcharts-angular": "^4.0.0",
    "jquery": "^3.7.1",
    "moment": "^2.30.1",
    "ngx-bootstrap": "^12.0.0",
    "ngx-histaff-alpha": "^1.8.1",
    "ngx-slick-carousel": "^17.0.0",
    "rxjs": "~7.8.0",
    "slick-carousel": "^1.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular/cli": "^17.0.10",
    "@angular/compiler-cli": "^17.0.0",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-selection": "^3.0.10",
    "@types/d3-shape": "^3.1.6",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lite-server": "^2.6.1",
    "ng-packagr": "^17.2.0",
    "typescript": "~5.2.2"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "url": false,
    "util": false,
    "stream": false,
    "querystring": false,
    "http": false,
    "https": false,
    "net": false,
    "crypto": false,
    "zlib": false
  }
}
